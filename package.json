    "prerelease-minor": "git checkout dev && npm version preminor && git push origin dev --tags",
    "prerelease-major": "git checkout dev && npm version premajor && git push origin dev --tags",
    "release-patch": "git checkout live && npm version patch && git push origin live --tags",
    "release-minor": "git checkout live && npm version minor && git push origin live --tags",
    "release-major": "git checkout live && npm version major && git push origin live --tags"
